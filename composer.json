{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "^7.29",
        "laravelcollective/html": "^6.1",
        "doctrine/dbal": "^2.6",
        "anahkiasen/former": "^4.4",
        "intervention/image": "~2.3.0",
        "misd/linkify": "~1.1.2",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "laracasts/presenter": "~0.2.1",
        "pda/pheanstalk": "^4.0",
        "msurguy/honeypot": "^1.1",
        "mailchimp/marketing": "*",
        "erusev/parsedown": "~1.7.2",
        "haampie/gravatar": "~1.4.0",
        "predis/predis": "~1.0",
        "laracasts/flash": "^3.1",
        "league/csv": "^8.0",
        "league/fractal": "^0.13.0",
        "fruitcake/laravel-cors": "*",
        "maatwebsite/excel": "^3.1",
        "giggsey/libphonenumber-for-php": "^7.4",
        "bugsnag/bugsnag-laravel": "^2.19",
        "algolia/algoliasearch-client-php": "^1.10",
        "php": "^7.2.5|^8.0",
        "laravel/tinker": "^2.5",
        "fideloper/proxy": "^4.4",
        "mews/purifier": "*",
        "laravel/ui": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "phpspec/phpspec": "*",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "fakerphp/faker": "*",
        "facade/ignition": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:haampie/gravatar.git"
        }
    ],
    "autoload": {
        "classmap": [
            "app/Http/Controllers",
            "app/Console/Commands",
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "@php artisan optimize"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "@php artisan optimize"
        ],
        "post-create-project-cmd": [
            "@php -r \"copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    }
}
